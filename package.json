{
  "name": "cozy-tasky",
  "displayName": "Tasky",
  "version": "1.1.3",
  "engines": [
    "node = 0.10.x"
  ],
  "description": "Simple and reactive task manager based on tagging.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsilvestre/tasky.git"
  },
  "bin": {
    "tasky": "bin/tasky"
  },
  "main": "build/server.js",
  "dependencies": {
    "americano": "0.3.9",
    "americano-cozy-pouchdb": "0.3.5",
    "async": "0.9.0",
    "jade": "1.3.1",
    "printit": "0.1.3",
    "path-extra": "0.2.1"
  },
  "devDependencies": {
    "should": "3.1.2",
    "mocha": "1.17.1",
    "request-json": "0.4.6",
    "cozy-fixtures": "1.1.0",
    "coffeelint": "1.1.0"
  },
  "scripts": {
    "start": "node build/server.js"
  },
  "cozy-permissions": {
    "Tasky": {
      "description": "Create, edit, organize all your tasks."
    },
    "CozyInstance": {
      "description": "This information is needed to localize the application in your language."
    }
  },
  "cozy-displayName": "Tasky"
}
